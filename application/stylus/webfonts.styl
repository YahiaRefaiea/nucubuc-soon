        url("../fonts/Intro/Intro-Bold-Alt.woff2") format('woff2'),
        url("../fonts/Intro/Intro-Bold-Alt.woff") format('woff'),
        url("../fonts/Intro/Intro-Bold-Alt.ttf") format('truetype'),
        url("../fonts/Intro/Intro-Bold-Alt.svg#Intro-Bold-Alt") format('svg')

@font-face
  font-family: 'Intro'
  font-weight: 900
  font-style: normal
