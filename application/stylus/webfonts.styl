//  INTRO
@font-face
  font-family: 'Intro'
  font-weight: 500
  font-style: normal
  src:  url("../fonts/Intro/Intro-Regular-Alt.eot")
  src:  url("../fonts/Intro/Intro-Regular-Alt.eot?#iefix") format('embedded-opentype'),
        url("../fonts/Intro/Intro-Regular-Alt.woff2") format('woff2'),
        url("../fonts/Intro/Intro-Regular-Alt.woff") format('woff'),
        url("../fonts/Intro/Intro-Regular-Alt.ttf") format('truetype'),
        url("../fonts/Intro/Intro-Regular-Alt.svg#Intro-Regular-Alt") format('svg')

@font-face
  font-family: 'Intro'
  font-weight: 700
  font-style: normal
